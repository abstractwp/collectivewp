{
  "name": "publishpress/publishpress-checklists-pro",
  "type": "wordpress-plugin",
  "description": "",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "PublishPress",
      "email": "help@publishpress.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PublishPress\\ChecklistsPro\\": "src",
      "PublishPress\\ChecklistsPro\\WooCommerce\\": "src/modules/woocommerce/lib",
      "PublishPress\\ChecklistsPro\\FeaturedImageSize\\": "src/modules/featured-image-size/lib"
    },
    "files": [
      "lib/wppf/includes.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:publishpress/PublishPress-Checklists.git"
    },
    {
      "type": "git",
      "url": "https://github.com/publishpress/PublishPress-Plugin-Builder"
    }
  ],
  "require": {
    "pimple/pimple": "3.2.3.*",
    "psr/container": "1.0.*",
    "symfony/polyfill-ctype": "1.19.*",
    "publishpress/publishpress-checklists": "2.7.4",
    "alledia/wordpress-edd-license-integration": "^2.3",
    "publishpress/publishpress-instance-protection": "^1.0"
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "require-dev": {
    "publishpress/publishpress-plugin-builder": "^1.3",
    "overtrue/phplint": "^2.1"
  },
  "scripts": {
    "build": "vendor/bin/robo build --ansi",
    "build-unpacked": "vendor/bin/robo build:unpacked --ansi",
    "test": [
      "vendor/bin/codecept run unit --bootstrap ./tests/unit/_bootstrap.php"
    ]
  }
}
